CLASS com.ibm.lconn.commom.operator.FilterExecOperator /com/ibm/lconn/migration/backup_20.filter

LOG ========== Starting importing data ===============

CONDITION NULL
CONDITION activities.installed true
CONDITION activities.source.data.exist true
#CONDITION activities.version 2.0.0
LOG ========== Activities =====================================
LOAD_WAS_VARIABLE ${activities.was.home} ${activities.profile.name} ${activities.cell.name} ${activities.node.name}
FILE_COPY migrationData/source/data/activities/content>>${ACTIVITIES_CONTENT_DIR}>>true>>true
FILE_COPY migrationData/source/data/activities/statistic>>${ACTIVITIES_STATS_DIR}>>true>>true

CONDITION NULL
CONDITION blogs.installed true
CONDITION blogs.source.data.exist true
#CONDITION blogs.version 2.0.0
LOG ========== Blogs ==========================================
LOAD_WAS_VARIABLE ${blogs.was.home} ${blogs.profile.name} ${blogs.cell.name} ${blogs.node.name}
FILE_COPY migrationData/source/data/blogs/upload>>${BLOGS_CONTENT_DIR}>>true>>true


CONDITION NULL
CONDITION communities.installed true
CONDITION communities.source.data.exist true
#CONDITION communities.version 2.0.0
LOG ========== Communities ====================================
LOG Nothing. 

CONDITION NULL
CONDITION dogear.installed true
CONDITION dogear.source.data.exist true
#CONDITION dogear.version 2.0.0
LOG ========== Dogear =========================================
LOG Nothing. 

CONDITION NULL
CONDITION profiles.installed true
CONDITION profiles.source.data.exist true
#CONDITION profiles.version 2.0.0
LOG ========== Profiles =======================================
LOG Nothing. 

CONDITION NULL
CONDITION homepage.installed true
CONDITION homepage.source.data.exist true
#CONDITION homepage.version 2.0.0
LOG ========== Homepage =======================================
LOG Nothing.

CONDITION NULL
LOG ========== Importing data completed =================