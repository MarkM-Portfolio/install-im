	<iu version='6.0.0.qualifier' adapterId='native' id='unzipVersionNews_iu'>
		<selectedBy id='news'/>
		<adapterSpecificData>
			<zip destination='${profile:installLocation}/version' id='connVersion_news' tolerance='[0.0.0,0.0.0]' overwrite='true'>
				<zipfileset dir='${version.work.dir}'>
