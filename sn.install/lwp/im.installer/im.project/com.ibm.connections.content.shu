	<iu version='6.0.0.0' adapterId='native' id='setConfig_iu'>
		<!--
		<property name='passive.order.dependencies' value='product_news_iu,product_search_iu,product_profiles_iu,product_profiles_iu,product_activities_iu,product_blogs_iu,product_communities_iu,product_dogear_iu,product_files_iu,product_forums_iu,product_homepage_iu,product_metrics_iu,product_wikis_iu,product_mobile_iu,product_moderation_iu,product_ccm_iu'/>
		-->
		<property name='order.dependencies' value='appendPropertyFile_iu' />
		<selectedBy id='main'/>
		<adapterSpecificData>
			<createFile resolveString='true' file='${profile:user.connections.install.location}/cfg.py'>
				<line># Common settings</line>
				<line>common_options = {</line>
				<line>  "installationManagerMode": "true",</line>
				<line>  "installType": "nd",</line>
				<!-- <line>  "cell": "${profile:user.was.dmCellName}",</line> -->
				<line>  "port": "${profile:user.was.wasSoapPort}",</line>
				<line>  "profileName": "${profile:user.was.profileName}",</line>
				<line>  "SOAP_CONNECTOR_ADDRESS": "${profile:user.was.wasSoapPort}",</line>
				<line>  "webserverNode": "${profile:user.web.server.node}",</line>
				<line>  "webserver": "${profile:user.web.server.name}",</line>
				<line>  "connectionsAdminUser": "${profile:user.was.adminuser.id}",</line>
				<line>  "connectionsAdminPassword": "${decrypt:${profile:user.was.adminuser.password}}",</line>
				<line>  "dataDirectory": "${profile:user.contentStore.shared.path}",</line>
				<line>  "dataDirectoryLocal": "${profile:user.contentStore.local.path}",</line>
				<line>  "msgStoreHome": "${profile:user.messageStore.shared.path}",</line>
				<line>  "msgStore": {</line>
				<line>    "logSize": 100,</line>
				<line>    "minPermanentStoreSize": 200,</line>
				<line>    "minTemporaryStoreSize": 200,</line>
				<line>    "maxPermanentStoreSize": 500,</line>
				<line>    "maxTemporaryStoreSize": 500</line>
				<line>  },</line>
				<line>  "lcHome": "${profile:user.connections.install.location}",</line>
				<line>  "enableWPI": "true",</line>
				<line>  "enableWCI": "true",</line>
				<line>  "replaceEAR": "true",</line>
				<line>  "disableTAMTAI": "true",</line>
				<line>  "disableSpnegoTAI": "true",</line>
				<line>  ${profile:user.notification.enabled.key.value}</line>
				<line>  "mailSession": {</line>
				<line>    ${profile:user.notification.port.key.value}</line>
				<line>    ${profile:user.notification.user.key.value}</line>
				<line>    ${profile:user.notification.password.key.value}</line>
				<line>    ${profile:user.notification.protocol.key.value}</line>
				<line>    "host": "${profile:user.notification.host}"</line>
				<line>    #"properties": {</line>
				<line>    #  "mail.smtp.connectiontimeout":12000</line>
				<line>    #}</line>
				<line>  },</line>
				<line>  ${profile:user.notification.replyto.info} </line>
				<line>  ${profile:user.notification.dns.url.key.value}</line>
				<line>  "ssoDomain": "${profile:user.was.domainName}",</line>
				<line>  "dmhostname": "${profile:user.was.dmHostname}",</line>
				<line>  "dmcellname": "${profile:user.was.dmCellName}",</line>
				<line>  "admin": "${profile:user.user.mapping.administrative.users}",</line>
				<line>  "global-moderator": "${profile:user.user.mapping.global.moderater.users}",</line>
				<!--
				<line>  "customize-roles-maps": {</line>
				<line>    "admin": [ "Yes", "No", "${profile:user.was.adminuser.id}", "" ],</line>
				<line>    "search-admin": [ "Yes", "No", "${profile:user.was.adminuser.id}", "" ],</line>
				<line>    "widgets-admin": [ "Yes", "No", "${profile:user.was.adminuser.id}", "" ],</line>
				<line>  },</line>
				-->
				<line>  "notificationEmailDomain": "${profile:user.notification.domain}",</line>
				<line>  "security": {"com.ibm.websphere.security.disableGetTokenFromMBean": "true"},</line>
				<line>  "log_options":{</line>
				<line>    "maxNumLogFiles":30,</line>
				<line>    "rolloverType":"BOTH",</line>
				<line>    "rolloverPeriod":24,</line>
				<line>    "baseHour":24</line>
				<line>  },</line>
				<line>  "webContainer":{"HttpSessionCloneId":"someValue","Description":"put_a_description_here"},</line>
				<line>  "jvm":{"verboseModeGarbageCollection":"true"},</line>
				<line>  "dbType": "${profile:user.database.type}",</line>
				<line>  "dbServer": "localhost",</line>
				<line>  "dbPassword": "-",</line>
				<line>  "dbDriverPath": "/"</line>
				<line>}</line>
				<line>#</line>
				<line># When install, put component specific settings here, any settings find in</line>
				<line># this section will override the same settings from the common_options.</line>
				<line>#</line>
				<line># application specific settings:</line>
				<line>#   "disableModule": ['calendar.war', 'catalog.war', 'qkr.search.engine.jar'],</line>
				<line>#   "deleteModule": ['calendar.war', 'catalog.war', 'qkr.search.engine.jar'],</line>
				<line>#</line>
				<line>apps_options = {</line>
				<line>##feature-config##</line>
				<line>}</line>
				<line>clusters_options = {</line>
				<line>##cluster-config##</line>
				<line>}</line>
			</createFile>
		</adapterSpecificData>
	</iu>
	
	<iu version='6.0.0.0' adapterId='native' id='connectionsSQL_iu'>
		<selectedByExpression>
			<or>
				<selectedBy value='linux' id='os'/>
				<selectedBy value='aix' id='os'/>
				<selectedBy value='win32' id='os'/>
			</or>
		</selectedByExpression>
		<adapterSpecificData>
			<zip id='connections.sql' destination='${profile:installLocation}/xkit' overwrite='true'>
				<zipfileset dir='${lc.live.installer}/xkit'>
					<include name='connections.sql/**/*'/>
					<exclude name='connections.sql/**/fixup/*'/>
					<exclude name='connections.sql/**/fixup'/>
					<exclude name='connections.sql/**/db2_IBMi/**/*'/>
					<exclude name='connections.sql/**/db2_IBMi'/>
					<exclude name='connections.sql/**/db2_i/**/*'/>
					<exclude name='connections.sql/**/db2_i'/>
				</zipfileset>
			</zip>
		</adapterSpecificData>
	</iu>

