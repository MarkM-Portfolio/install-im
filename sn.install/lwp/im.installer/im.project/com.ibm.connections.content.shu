	<iu version='6.0.0.qualifier' adapterId='native' id='unzip_iu'>
		<selectedBy id='main'/>
		<adapterSpecificData>
			<perform duringPhases='pre_install_configure'>
				<delete file="xkit" deleteDirIfEmpty="false" />
				<delete file="lib/*.class" deleteDirIfEmpty="true" />
			</perform>
			<zip id='product_common' destination='${profile:installLocation}' overwrite='true'>
				<zipfileset dir='${lc.live.installer}'>
					<include name='xkit/**/*'/>
					<exclude name='xkit/connections.sql/**/db2_IBMi/**/*'/>
					<exclude name='xkit/connections.sql/**/db2_IBMi'/>
					<exclude name='xkit/connections.sql/**/db2_i/**/*'/>
					<exclude name='xkit/connections.sql/**/db2_i'/>
					<include name='bin/*'/>
					<include name='lib/**/*'/>
				</zipfileset>
			</zip>
			
			<!-- Per agreement with the Offering Team, we're not refreshing the CCM package for IC6.0, so we didn't request a new Library tag. 
			Customers will install 6.0 & continue to use the CCM 5.5 package, so the existing tag should suffice.  -->
			<zip id='connections.swidtag' destination='${profile:installLocation}/swidtag' overwrite='true'>
				<zipfileset dir='${iso.swid.dir}'>
					<include name='*.swidtag' />
					<exclude name='1986-03.com.ibm_IBM_Connections_-_Library-5.5.0.swidtag' />
				</zipfileset>
			</zip>
		</adapterSpecificData>
	</iu>
	
