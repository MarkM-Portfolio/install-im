type=`uname`
if [ "$type" = "AIX" ]; then
	res=`whoami`
fi

if [ "$type" = "HP-UX" ]; then
        res=`whoami`
fi

if [ "$type" = "Linux" ]; then
	res=`whoami`
fi

if [ "$type" = "SunOS" ]; then
	res=`/usr/ucb/whoami`
fi

if [ "$res" = "root" ]; then
	echo "$Msg_TRUE"
else
	echo "$Msg_FALSE"
fi
